<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="MissingDimension"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <FrameLayout
        android:background="@android:color/white"
        android:layout_gravity="center_vertical"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="60dp">
        <LinearLayout
            android:gravity="center"
            android:layout_width="60dp"
            android:layout_height="match_parent">
            <ImageButton
                android:contentDescription="@string/login_fragment_main_btn_back"
                android:background="@mipmap/backb"
                android:layout_gravity="center"
                android:scaleType="fitXY"
                android:layout_width="28dp"
                android:layout_height="28dp"
                android:id="@+id/btnBack"/>
        </LinearLayout>
        <TextView
            android:layout_marginLeft="60dp"
            android:text="@string/setting_activity_title"
            android:textSize="15dp"
            android:layout_marginRight="50dp"
            android:textColor="@color/textblackcolor"
            android:layout_gravity="center_vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/lnBack"
            android:layout_width="200dp"
            android:layout_height="match_parent"/>
    </FrameLayout>
    <View
        android:background="@color/linegraycolor"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"/>
    <ScrollView
        android:layout_weight="0.8"
        android:background="@android:color/white"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <!--merchantsetting-->
            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_marginLeft="15dp"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="15dp"
                    android:textColor="@color/textorange"
                    android:text="@string/merchantsetting"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"/>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:layout_height="60dp">
                    <ImageButton
                        android:layout_marginLeft="15dp"
                        android:background="@mipmap/settingmerchant"
                        android:scaleType="fitXY"
                        android:layout_gravity="center_vertical"
                        android:layout_width="28dp"
                        android:layout_height="28dp"/>
                    <TextView
                        android:textColor="@color/textblackcolor"
                        android:textSize="15dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="70dp"
                        android:text="@string/merchant"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <FrameLayout
                        android:background="@android:color/transparent"
                        android:id="@+id/btnMerchant"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                </FrameLayout>
                <View
                    android:background="@color/colorbackground"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"/>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:layout_height="60dp">
                    <ImageButton
                        android:layout_marginLeft="15dp"
                        android:background="@mipmap/settingbranch"
                        android:scaleType="fitXY"
                        android:layout_gravity="center_vertical"
                        android:layout_width="28dp"
                        android:layout_height="28dp"/>
                    <TextView
                        android:textColor="@color/textblackcolor"
                        android:textSize="15dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="70dp"
                        android:text="@string/branch"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <FrameLayout
                        android:background="@android:color/transparent"
                        android:id="@+id/btnBranch"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                </FrameLayout>
                <View
                    android:background="@color/colorbackground"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"/>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:layout_height="60dp">
                    <ImageButton
                        android:layout_marginLeft="15dp"
                        android:background="@mipmap/settingemployee"
                        android:scaleType="fitXY"
                        android:layout_gravity="center_vertical"
                        android:layout_width="28dp"
                        android:layout_height="28dp"/>
                    <TextView
                        android:textColor="@color/textblackcolor"
                        android:textSize="15dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="70dp"
                        android:text="@string/employeemanage"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <FrameLayout
                        android:background="@android:color/transparent"
                        android:id="@+id/btnEmployee"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                </FrameLayout>
                <View
                    android:background="@color/colorbackground"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"/>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:layout_height="60dp">
                    <ImageButton
                        android:layout_marginLeft="15dp"
                        android:background="@mipmap/settinglinkpromaxx"
                        android:scaleType="fitXY"
                        android:layout_gravity="center_vertical"
                        android:layout_width="28dp"
                        android:layout_height="28dp"/>
                    <TextView
                        android:textColor="@color/textblackcolor"
                        android:textSize="15dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="70dp"
                        android:text="@string/linkproMaxx"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <FrameLayout
                        android:background="@android:drawable/screen_background_light_transparent"
                        android:id="@+id/btnLinkProMaxx"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                </FrameLayout>
                <View
                    android:background="@color/colorbackground"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"/>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:layout_height="60dp">
                    <ImageButton
                        android:layout_marginLeft="15dp"
                        android:background="@mipmap/SettingPackage"
                        android:scaleType="fitXY"
                        android:layout_gravity="center_vertical"
                        android:layout_width="28dp"
                        android:layout_height="28dp"/>
                    <TextView
                        android:textColor="@color/textblackcolor"
                        android:textSize="15dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="70dp"
                        android:text="@string/settingpackage"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <FrameLayout
                        android:background="@android:color/transparent"
                        android:id="@+id/btnPackage"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                </FrameLayout>
                <View
                    android:background="@color/colorbackground"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"/>
            </LinearLayout>

            <!--itemsetting-->
            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_marginLeft="15dp"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="15dp"
                    android:textColor="@color/textorange"
                    android:text="@string/itemsetting"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"/>
                <FrameLayout
                    android:visibility="visible"
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:layout_height="60dp">
                    <ImageButton
                        android:layout_marginLeft="15dp"
                        android:background="@mipmap/settingnote"
                        android:scaleType="fitXY"
                        android:layout_gravity="center_vertical"
                        android:layout_width="28dp"
                        android:layout_height="28dp"/>
                    <TextView
                        android:textColor="@color/textblackcolor"
                        android:textSize="15dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="70dp"
                        android:text="@string/note"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <FrameLayout
                        android:background="@android:color/transparent"
                        android:id="@+id/btnNoteSetting"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                </FrameLayout>
                <View
                    android:background="@color/colorbackground"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"/>
            </LinearLayout>

            <!--itemsetting-->
            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_marginLeft="15dp"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="15dp"
                    android:textColor="@color/textorange"
                    android:text="@string/membersetting"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"/>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:layout_height="60dp">
                    <ImageButton
                        android:layout_marginLeft="15dp"
                        android:background="@mipmap/settingmembertype"
                        android:scaleType="fitXY"
                        android:layout_gravity="center_vertical"
                        android:layout_width="28dp"
                        android:layout_height="28dp"/>
                    <TextView
                        android:textColor="@color/textblackcolor"
                        android:textSize="15dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="70dp"
                        android:text="@string/membertype"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <FrameLayout
                        android:background="@android:color/transparent"
                        android:id="@+id/btnMembertype"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                </FrameLayout>
                <View
                    android:background="@color/colorbackground"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"/>
            </LinearLayout>

            <!--billsetting-->
            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_marginLeft="15dp"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="15dp"
                    android:textColor="@color/textorange"
                    android:text="@string/billsetting"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"/>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:layout_height="60dp">
                    <ImageButton
                        android:layout_marginLeft="15dp"
                        android:background="@mipmap/settingvat"
                        android:scaleType="fitXY"
                        android:layout_gravity="center_vertical"
                        android:layout_width="28dp"
                        android:layout_height="28dp"/>
                    <LinearLayout
                        android:layout_marginLeft="70dp"
                        android:orientation="vertical"
                        android:gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent">
                        <TextView
                            android:textColor="@color/textblackcolor"
                            android:textSize="15dp"
                            android:layout_gravity="center_vertical"
                            android:text="@string/vat"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:id="@+id/txtVat"
                            android:textColor="@color/secondaryText"
                            android:textSize="15dp"
                            android:layout_gravity="center_vertical"
                            android:text="7%"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>

                    <FrameLayout
                        android:background="@android:color/transparent"
                        android:id="@+id/btnVat"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                </FrameLayout>
                <View
                    android:background="@color/colorbackground"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"/>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:layout_height="60dp">
                    <ImageButton
                        android:layout_marginLeft="15dp"
                        android:background="@mipmap/settingcurrency"
                        android:scaleType="fitXY"
                        android:layout_gravity="center_vertical"
                        android:layout_width="28dp"
                        android:layout_height="28dp"/>
                    <LinearLayout
                        android:layout_marginLeft="70dp"
                        android:orientation="vertical"
                        android:gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent">
                        <TextView
                            android:textColor="@color/textblackcolor"
                            android:textSize="15dp"
                            android:layout_gravity="center_vertical"
                            android:text="@string/currency"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:id="@+id/txtCurrency"
                            android:textColor="@color/secondaryText"
                            android:textSize="15dp"
                            android:layout_gravity="center_vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>

                    <FrameLayout
                        android:background="@android:color/transparent"
                        android:id="@+id/btnCurrency"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                </FrameLayout>
                <View
                    android:background="@color/colorbackground"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"/>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:layout_height="60dp">
                    <ImageButton
                        android:layout_marginLeft="15dp"
                        android:background="@mipmap/settingdecimal"
                        android:scaleType="fitXY"
                        android:layout_gravity="center_vertical"
                        android:layout_width="28dp"
                        android:layout_height="28dp"/>
                    <TextView
                        android:textColor="@color/textblackcolor"
                        android:textSize="15dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="70dp"
                        android:text="@string/decimal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <FrameLayout
                        android:background="@android:color/transparent"
                        android:id="@+id/btnDecimal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                </FrameLayout>
                <View
                    android:background="@color/colorbackground"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"/>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:layout_height="60dp">
                    <ImageButton
                        android:layout_marginLeft="15dp"
                        android:background="@mipmap/settingservicecharge"
                        android:scaleType="fitXY"
                        android:layout_gravity="center_vertical"
                        android:layout_width="28dp"
                        android:layout_height="28dp"/>

                    <LinearLayout
                        android:layout_marginLeft="70dp"
                        android:orientation="vertical"
                        android:gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent">
                        <TextView
                            android:textColor="@color/textblackcolor"
                            android:textSize="15dp"
                            android:layout_gravity="center_vertical"
                            android:text="@string/servicecharge"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:id="@+id/txtServiceCharge"
                            android:textColor="@color/secondaryText"
                            android:textSize="15dp"
                            android:layout_gravity="center_vertical"
                            android:text="฿ 10.00"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>

                    <FrameLayout
                        android:background="@android:color/transparent"
                        android:id="@+id/btnServiceCharge"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                </FrameLayout>
                <View
                    android:background="@color/colorbackground"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"/>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:layout_height="60dp">
                    <ImageButton
                        android:layout_marginLeft="15dp"
                        android:background="@mipmap/CurrencyUSD"
                        android:scaleType="fitXY"
                        android:layout_gravity="center_vertical"
                        android:layout_width="28dp"
                        android:layout_height="28dp"/>

                    <LinearLayout
                        android:layout_marginLeft="70dp"
                        android:orientation="vertical"
                        android:gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent">
                        <TextView
                            android:textColor="@color/textblackcolor"
                            android:textSize="15dp"
                            android:layout_gravity="center_vertical"
                            android:text="@string/settingcash"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>

                    <FrameLayout
                        android:background="@android:color/transparent"
                        android:id="@+id/btnCash"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                </FrameLayout>
                <View
                    android:background="@color/colorbackground"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"/>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:layout_height="60dp">
                    <ImageButton
                        android:layout_marginLeft="15dp"
                        android:background="@mipmap/settinggiftvoucher"
                        android:scaleType="fitXY"
                        android:layout_gravity="center_vertical"
                        android:layout_width="28dp"
                        android:layout_height="28dp"/>
                    <TextView
                        android:textColor="@color/textblackcolor"
                        android:textSize="15dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="70dp"
                        android:text="@string/giftvoucher"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <FrameLayout
                        android:background="@android:color/transparent"
                        android:id="@+id/btnGiftvoucher"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                </FrameLayout>
                <View
                    android:background="@color/colorbackground"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"/>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:layout_height="60dp">
                    <ImageButton
                        android:layout_marginLeft="15dp"
                        android:background="@mipmap/historyqr"
                        android:scaleType="fitXY"
                        android:layout_gravity="center_vertical"
                        android:layout_width="28dp"
                        android:layout_height="28dp"/>
                    <TextView
                        android:textColor="@color/textblackcolor"
                        android:textSize="15dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="70dp"
                        android:text="@string/settingmyqr"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <FrameLayout
                        android:background="@android:color/transparent"
                        android:id="@+id/btnMYQRSetting"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                </FrameLayout>
                <View
                    android:background="@color/colorbackground"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"/>
            </LinearLayout>

            <!--devicesetting-->
            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_marginLeft="15dp"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="15dp"
                    android:textColor="@color/textorange"
                    android:text="@string/devicesetting"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"/>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:layout_height="60dp">
                    <ImageButton
                        android:layout_marginLeft="15dp"
                        android:background="@mipmap/settingdevice"
                        android:scaleType="fitXY"
                        android:layout_gravity="center_vertical"
                        android:layout_width="28dp"
                        android:layout_height="28dp"/>
                    <TextView
                        android:textColor="@color/textblackcolor"
                        android:textSize="15dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="70dp"
                        android:text="@string/device"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <FrameLayout
                        android:background="@android:color/transparent"
                        android:id="@+id/btnDevice"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                </FrameLayout>
                <View
                    android:background="@color/colorbackground"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"/>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:layout_height="60dp">
                    <ImageButton
                        android:layout_marginLeft="15dp"
                        android:background="@mipmap/settingprinter"
                        android:scaleType="fitXY"
                        android:layout_gravity="center_vertical"
                        android:layout_width="28dp"
                        android:layout_height="28dp"/>
                    <TextView
                        android:textColor="@color/textblackcolor"
                        android:textSize="15dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="70dp"
                        android:text="@string/printer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <FrameLayout
                        android:background="@android:color/transparent"
                        android:id="@+id/btnPrinter"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                </FrameLayout>
                <View
                    android:background="@color/colorbackground"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"/>
            </LinearLayout>
        </LinearLayout>

    </ScrollView>

</LinearLayout>
